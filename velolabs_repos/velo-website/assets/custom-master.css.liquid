@font-face {
  font-family: 'helvetica_neueregular';
  src: url('helveticaneue-thin-webfont.eot');
  src: url('helveticaneue-thin-webfont.eot?#iefix') format('embedded-opentype'),
    url('helveticaneue-thin-webfont.woff2') format('woff2'),
    url('helveticaneue-thin-webfont.woff') format('woff'),
    url('helveticaneue-thin-webfont.ttf') format('truetype'),
    url('helveticaneue-thin-webfont.svg#helvetica_neueregular') format('svg');
  font-weight: normal;
  font-style: normal;

}
@font-face {
    font-family: Gotham-Bold;
    src: url(Gotham-Bold.ttf);
}
@font-face {
    font-family: Gotham-Light;
    src: url(Gotham-Light.ttf);
}
@font-face {
    font-family: Gotham-Medium;
    src: url(Gotham-Medium.ttf);
}


.home-top-section p {
  font-family: proxima_novalight;
  font-size: 18px;
  max-width: 500px;
  width: 100%;
  text-align: center; 
  margin: 0 auto;
}

.home-top-section p.text-note {
  font-size: 12px;
  margin: 10px auto !important;
  color: #000;
  font-family: Gotham-Medium;
}

.home-top-section h2 {
  color: #000;
  font-size: 17px;
  font-weight: bold;
  line-height: 1.4em;
  margin: 0 auto 5px;
  max-width: 460px;
}

.home-top-section span.price-par {
  color: #1a5c96;
  display: inline-block;
  width: 100%;
  font-size: 26px;
  margin: 0 auto 5px;
  font-family: Gotham-Bold;
}

.home-top-section span.price-par strike{
  margin-right: 5px;

}

.home-top-section a {

}

.button.preorder.clicked-preorder.slider-prebutton {
  font-size: 15px;
  font-weight: normal;
  height: 42px;
  line-height: 42px;
  width: 220px;
  background-image: -moz-linear-gradient( 113deg, rgb(15,61,101) 0%, rgb(30,108,176) 100%);
  background-image: -webkit-linear-gradient( 113deg, rgb(15,61,101) 0%, rgb(30,108,176) 100%);
  background-image: -ms-linear-gradient( 113deg, rgb(15,61,101) 0%, rgb(30,108,176) 100%);
  box-shadow: 0px 0px 11.18px 1.82px rgba(255, 255, 255, 0.54);
}


.home-top-section-wrapper{
  text-align: center;
}


.home-top-section .column-2 {
  display: table-cell;
  float: none!important;
  vertical-align: middle;
  width: 50%;
}

.home-top-section{
  display: table;
  margin: 0 auto;
  max-width: 1200px;
  padding: 0 50px 50px;
  width: 100%;
  overflow: hidden;
}

.home-brands .home-full-width-feature.home-module {
  display: table;
  margin: 0 auto;
  max-width: 1453px;
  padding: 85px 50px 85px;
  width: 100%;
}

.img-small-show{
  display: none; 
}


.home-top-section .imgpart img {
  float: none;
  display: block;
  margin: 0px auto;
  max-width: 900px;
  width: 100%; 
  background-color: #fff;

}

.main-header-wrapper {
  height: auto !important;
  padding-bottom: 11px !important;
  padding-top: 10px !important;
}


.sticky-header-bg {
  background-color: #fff;
  border-bottom: 1px solid #eee;
}

.home-brands .brands .brands-ul {
  display: table;
  list-style: outside none none;
  margin: 0;
  min-height: 80px;
  text-align: left;
  width: 100%;
}

.home-brands .brands .brands-ul .brand-li {
  display: table-cell;
  margin: 0;
  min-height: 0;
  vertical-align: middle;
  width: 14.2857%;
}

.home-brands .brands .brands-ul .brand-li a {
  display: table;
  float: none;
  text-align: center;
  margin: 0 auto;
}

.slider-prebutton{
  background-color: #1f6bb0; 
}

.slider-prebutton:hover{
  background-color: #103d65;
}

.home-top-section .button.videobutton{
  margin-left: 30px;
  background-color: transparent;
  border: 2px solid #103d65;
  border-radius: 23px;
  color: #103d65;
  font-size: 15px;
  font-weight: normal;
  height: 42px;
  text-align: center;
  width: 220px;
  letter-spacing: 0;
  display: inline-block !important;
  vertical-align: middle !important;
  padding: 0!important;
}

.home-top-section .button.videobutton span {
  display: inline-block !important;
  float: none;
  line-height: 38px;
  margin: 0 5px;
  text-align: center !important;
  vertical-align: top !important;
  
  font-family: Montserrat;
  font-size: 14px;
}
span.videobutton-icon{
  background-image: url({{ "play-black.png" | asset_url }});
  background-color: transparent;
  background-position: center center;
  background-repeat: no-repeat;
  background-size: 100% auto;
  display: inline-block;
  height: 38px;
  text-align: center;
  vertical-align: middle;
  width: 20px;
}

span.na-shipping {
  display: inline-block;
  font-size: 16px;
  margin: 0 0 0 5px;
  vertical-align: middle;
  color: #000;
  
  font-family: Gotham-Medium;
}

.template-index .main-content {
  max-width: 100%;
  padding-top: 0;
}

html, .footer-wrapper{
  background-color: #1e6ab0;
  overflow-x: hidden;
}

footer .mailing-list-submit input.submit {
  font-weight: normal; 
}

.footer-wrapper .mailing-list-submit .submit.secondary{
  background: #3498db;
}


.home-brands .brands .brands-ul .brand-li img{
  height: auto;  
}

a.logo img{
  height: 40px;
}

footer .first-row p{
  vertical-align: middle; 
  color: #fff;
}

footer .first-row .logo-footer {
  display: inline-block;
  float: none;
  padding-right: 5px;
  vertical-align: middle;
  width: 26px;
}

.has-retina .logo-retina{
  height: 40px; 
}

