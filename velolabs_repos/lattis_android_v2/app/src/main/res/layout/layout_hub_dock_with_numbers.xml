<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:custom="http://schemas.android.com/tools">


    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:src="@drawable/hub_dock_bike"
        android:id="@+id/iv_hub_dock_main"
        />


    <ImageView
        android:id="@+id/iv_hub_dock_number"
        android:layout_width="14dp"
        android:layout_height="14dp"
        android:src="@drawable/hub_dock_bike_number"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.lattis.lattis.presentation.customview.CustomTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="10dp"
        android:text="8"
        android:id="@+id/tv_hub_dock_number"
        app:layout_constraintTop_toTopOf="@+id/iv_hub_dock_number"
        app:layout_constraintBottom_toBottomOf="@+id/iv_hub_dock_number"
        app:layout_constraintEnd_toEndOf="@+id/iv_hub_dock_number"
        app:layout_constraintStart_toStartOf="@+id/iv_hub_dock_number"
        android:textColor="@color/hub_dock_bike"
        custom:typefaceAsset="GothamSSm-Book.otf"/>
</androidx.constraintlayout.widget.ConstraintLayout>