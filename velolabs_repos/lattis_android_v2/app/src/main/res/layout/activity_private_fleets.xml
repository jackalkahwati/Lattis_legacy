<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >

    <ViewFlipper
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="visible"
        android:id="@+id/view_flipper_in_private_fleet">
        <include layout="@layout/activity_private_fleets_no_fleets" />
        <include layout="@layout/activity_private_fleets_fleet_list"/>
    </ViewFlipper>



    <include layout="@layout/activity_loading"
        android:id="@+id/private_fleet_activity_loading_view"
        android:visibility="gone"/>

    <include layout="@layout/no_internal_layout"/>
</androidx.constraintlayout.widget.ConstraintLayout>