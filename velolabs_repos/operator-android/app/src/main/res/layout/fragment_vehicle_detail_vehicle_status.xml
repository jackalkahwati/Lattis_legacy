<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/round_vehicle_detail_box"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:custom="http://schemas.android.com/apk/res-auto">


    <io.lattis.operator.presentation.customview.CustomTextView
        android:id="@+id/ct_vehicle_status_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="15dp"
        android:text="@string/status"
        android:textSize="15dp"
        android:paddingBottom="15dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        custom:typefaceAsset="GothamSSm-Medium.otf" />
    <io.lattis.operator.presentation.customview.CustomTextView
        android:id="@+id/ct_vehicle_usage_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="20dp"
        android:textSize="15dp"
        android:textStyle="bold"
        android:paddingBottom="15dp"
        android:textColor="@color/vehicle_box_text"
        app:layout_constraintBottom_toBottomOf="@+id/ct_vehicle_status_value"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/ct_vehicle_status_value"
        custom:typefaceAsset="GothamSSm-Bold.otf" />



</androidx.constraintlayout.widget.ConstraintLayout>