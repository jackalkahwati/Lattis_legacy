<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">


    <io.lattis.operator.presentation.customview.CustomTextView
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginLeft="20dp"
        android:id="@+id/ct_vendor_other_equipment"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="16dp"
        android:textColor="@color/lattis_profile_accent"
        app:typefaceAsset="GothamSSm-Book.otf"
        />


    <io.lattis.operator.presentation.customview.CustomTextView
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginRight="20dp"
        android:id="@+id/ct_device_type_other_equipment"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="16dp"
        app:typefaceAsset="GothamSSm-Book.otf"
        />

    <View
        android:id="@+id/divider_in_other_equipment"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginTop="20dp"
        android:background="@color/home_fleet_in_divider"
        app:layout_constraintTop_toBottomOf="@+id/ct_device_type_other_equipment" />


</androidx.constraintlayout.widget.ConstraintLayout>