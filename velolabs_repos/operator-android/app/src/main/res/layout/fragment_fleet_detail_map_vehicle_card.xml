<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:custom="http://schemas.android.com/apk/res-auto"
    app:layout_constraintBottom_toBottomOf="parent"
    android:layout_marginBottom="20dp"
    android:layout_marginRight="20dp"
    android:layout_marginLeft="20dp"
    >

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/navigation"
        android:id="@+id/iv_vehicle_navigation"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:background="@drawable/round_vehicle_card"
        app:layout_constraintTop_toBottomOf="@+id/iv_vehicle_navigation"
        >

    <io.lattis.operator.presentation.customview.CustomTextView
        android:id="@+id/ct_vehicle_name_value_in_vehicle_card"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:textSize="15dp"
        android:textStyle="bold"
        android:textColor="@color/vehicle_box_text"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="10dp"
        custom:typefaceAsset="GothamSSm-Bold.otf" />

    <io.lattis.operator.presentation.customview.CustomTextView
        android:id="@+id/ct_vehicle_status_value_in_vehicle_card"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="15dp"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginRight="20dp"
        app:layout_constraintTop_toTopOf="@id/ct_vehicle_name_value_in_vehicle_card"
        app:layout_constraintBottom_toBottomOf="@id/ct_vehicle_name_value_in_vehicle_card"
        custom:typefaceAsset="GothamSSm-Medium.otf" />

    <io.lattis.operator.presentation.customview.CustomTextView
        android:id="@+id/ct_vehicle_type_value_in_vehicle_card"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="15dp"
        android:layout_marginLeft="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ct_vehicle_name_value_in_vehicle_card"
        android:layout_marginTop="10dp"
        android:paddingBottom="10dp"
        custom:typefaceAsset="GothamSSm-Medium.otf" />

    <io.lattis.operator.presentation.customview.CustomTextView
        android:id="@+id/ct_vehicle_usage_value_in_vehicle_card"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="20dp"
        android:textSize="15dp"
        android:textStyle="bold"
        android:paddingBottom="15dp"
        android:layout_marginTop="10dp"
        android:textColor="@color/vehicle_box_text"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/ct_vehicle_name_value_in_vehicle_card"
        custom:typefaceAsset="GothamSSm-Bold.otf" />

    </androidx.constraintlayout.widget.ConstraintLayout>




</androidx.constraintlayout.widget.ConstraintLayout>