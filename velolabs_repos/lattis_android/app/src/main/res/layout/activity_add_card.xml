<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.lattis.ellipse.presentation.ui.damagebike.DamageBikeActivity">

    <com.google.android.material.appbar.AppBarLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:elevation="0dp"
        android:id="@+id/appbarlayout"
        android:theme="@style/AppTheme.AppBarOverlay">
        <include layout="@layout/view_toolbar_child" />
    </com.google.android.material.appbar.AppBarLayout>

    <include
        layout="@layout/content_add_card" />

    <include layout="@layout/view_loading_fragment" android:visibility="gone" />

    <include layout="@layout/no_internal_layout"/>

</androidx.coordinatorlayout.widget.CoordinatorLayout>